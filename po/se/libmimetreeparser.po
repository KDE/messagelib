#: src/messagepart.cpp:702 src/messagepart.cpp:735 src/messagepart.cpp:1065
#, kde-format
msgid "Wrong Crypto Plug-In."
msgstr "Boasttu krypteren-lassemodula."

#: src/messagepart.cpp:793 src/messagepart.cpp:1273
#, kde-format
msgid "No appropriate crypto plug-in was found."
msgstr "Ii gávdnan heivvolaš krypterenlassemodula."

#: src/messagepart.cpp:795
#, kde-format
msgctxt "%1 is either 'OpenPGP' or 'S/MIME'"
msgid "No %1 plug-in was found."
msgstr "%1 lassemodula ii gávdnon."

#: src/messagepart.cpp:798
#, kde-format
msgid "Crypto plug-in \"%1\" cannot verify signatures."
msgstr "Krypterenlassemodula «%1» ii sáhte nannet vuolláičállagii."

#: src/messagepart.cpp:801
#, kde-format
msgid ""
"The message is signed, but the validity of the signature cannot be verified."
"<br />Reason: %1"
msgstr ""
"Reive lea vuolláičállon, muhto ii sáhte geahččalit jos vuolláičála gusto."
"<br />Sivva lea: %1"

#: src/messagepart.cpp:1275
#, kde-format
msgid "Crypto plug-in \"%1\" cannot decrypt messages."
msgstr "Krypterenlassemodula «%1» ii sáhte dekrypteret reivviid."

#: src/messagepart.cpp:1277
#, kde-format
msgid "Crypto plug-in \"%1\" could not decrypt the data."
msgstr "Krypteren lassemodula %1 ii sáhttán dekrypteret dataid."

#: src/messagepart.cpp:1278
#, kde-format
msgid "Error: %1"
msgstr "Meattáhus: %1"
