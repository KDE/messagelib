#. i18n: ectx: property (whatsThis), widget (QLineEdit, lineEdit_quote)
#: ui/templatesconfiguration_base.ui:326
#, no-c-format, kde-format
msgctxt "@info:whatsthis"
msgid ""
"\n"
"             <qt>The following placeholders are supported in the quote "
"indicator:\n"
"             <ul>\n"
"             <li>%f: sender's initials</li>\n"
"             <li>%%: percent sign</li>\n"
"             <li>%_: space</li>\n"
"             </ul></qt>\n"
"           "
msgstr ""
"\n"
"          <qt>Le sequente substitutos es supportate in le indicator de "
"citation;\n"
"          <ul>\n"
"          <li>%f: initiales de mittente</li>\n"
"          <li>%%: signo de percentage</li>\n"
"          <li>%_: spatio</li>\n"
"         </ul></qt>\n"
"        "

